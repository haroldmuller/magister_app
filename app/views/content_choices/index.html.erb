<p id="notice"><%= notice %></p>

<h1>Listing Content Choices</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Right</th>
      <th>Content question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @content_choices.each do |content_choice| %>
      <tr>
        <td><%= content_choice.text %></td>
        <td><%= content_choice.right %></td>
        <td><%= content_choice.content_question %></td>
        <td><%= link_to 'Show', content_choice %></td>
        <td><%= link_to 'Edit', edit_content_choice_path(content_choice) %></td>
        <td><%= link_to 'Destroy', content_choice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content choice', new_content_choice_path %>
