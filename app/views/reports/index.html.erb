
<h1>Listing Reports</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Interpretation sc</th>
      <th>Analysis sc</th>
      <th>Evaluation sc</th>
      <th>Inference sc</th>
      <th>Explanation sc</th>
      <th>Selfregulation sc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.course %></td>
        <td><%= report.interpretation_sc %></td>
        <td><%= report.analysis_sc %></td>
        <td><%= report.evaluation_sc %></td>
        <td><%= report.inference_sc %></td>
        <td><%= report.explanation_sc %></td>
        <td><%= report.selfregulation_sc %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_report_path %>
