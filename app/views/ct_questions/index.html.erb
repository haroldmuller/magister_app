<p id="notice"><%= notice %></p>

<h1>Listing Ct Questions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Tree</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ct_questions.each do |ct_question| %>
      <tr>
        <td><%= ct_question.question %></td>
        <td><%= ct_question.tree %></td>
        <td><%= link_to 'Show', ct_question %></td>
        <td><%= link_to 'Edit', edit_ct_question_path(ct_question) %></td>
        <td><%= link_to 'Destroy', ct_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ct question', new_ct_question_path %>
