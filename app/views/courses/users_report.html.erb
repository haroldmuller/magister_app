<% if current_user.role?%>
  <h1>Rendimiento de alumnos de <%= @course.name %></h1>
  <br></br>

  <br></br>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Alumno</th>
        <th>Contenido</th>
        <th>Interpretación</th>
        <th>Análisis</th>
        <th>Evaluación</th>
        <th>Inferencia</th>
        <th>Explicación</th>
        <th>Autoregulación</th>
      </tr>
    </thead>
    <tbody>
      <% puts "antes deeeeel usuarios inspeeect-------------------" %>
      <% puts @users_sc.inspect %>
      <% @users_sc.each do |key, value| %>
        <tr>            
            <td><%= value[:name] %></td>
            <% if !value[:content_sc].nil? %>
            <td><%= value[:content_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:interpretation_sc].nil? %>
            <td><%= value[:interpretation_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:analysis_sc].nil? %>
            <td><%= value[:analysis_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:evaluation_sc].nil? %>
            <td><%= value[:evaluation_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:inference_sc].nil? %>
            <td><%= value[:inference_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:explanation_sc].nil? %>
            <td><%= value[:explanation_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            <% if !value[:selfregulation_sc].nil? %>
            <td><%= value[:selfregulation_sc] %></td>
            <% else %>
            <td> N/O </td>
            <% end %>
            
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
