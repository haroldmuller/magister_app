    <div class="panel-body">       
       <%= link_to tree.content.text, course_tree_path(@course, tree) %>       
       <p><%= link_to 'Eliminar', [@course, tree], method: :delete, data: { confirm: 'Â¿Seguro que deseas eliminar el arbol?' }, :class=>"btn btn-primary pull-right", :style=>'color:white' %></p>
       <div class="clearfix"></div>
       <p><%= link_to 'Editar', edit_course_tree_path(@course, tree), :class=>"btn btn-primary pull-right", :style=>'color:white'%></p>
    </div>
  </div>
<% end %>

