<p id="notice"><%= notice %></p>

<h1>Listing Ct Choices</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Right</th>
      <th>Ct question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ct_choices.each do |ct_choice| %>
      <tr>
        <td><%= ct_choice.text %></td>
        <td><%= ct_choice.right %></td>
        <td><%= ct_choice.ct_question %></td>
        <td><%= link_to 'Show', ct_choice %></td>
        <td><%= link_to 'Edit', edit_ct_choice_path(ct_choice) %></td>
        <td><%= link_to 'Destroy', ct_choice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ct choice', new_ct_choice_path %>
